Версию JDK  - openjdk 10.0.2 2018-07-17

Чтобы запустить генерацию XLSX и PDF, нужно сделать следущие шаги:
1) попасть в папку fintech2018/task3/src/main/java
2) javac TestFileCreater.java
3) java TestFileCreater

create database fintech
CREATE USER 'fintech'@'localhost' IDENTIFIED BY '123456';
GRANT ALL PRIVILEGES ON `fintech`.* TO 'fintech'@'localhost';
FLUSH PRIVILEGES;

CREATE TABLE `testInfo` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `fname` varchar(100) NOT NULL,
  `lname` varchar(100) NOT NULL,
  `patronymic` varchar(100) NOT NULL,
  `city` varchar(100) NOT NULL,
  `street` varchar(100) NOT NULL,
  `gender` varchar(10) NOT NULL,
  `date` date NOT NULL,
  `postcode` varchar(100) NOT NULL,
  `house` int(11) NOT NULL,
  `apartment` int(11) NOT NULL,
  `phone` varchar(100) NOT NULL DEFAULT '',
  `login` varchar(100) NOT NULL DEFAULT '',
  `password` varchar(100) NOT NULL DEFAULT '',
  `userpic` varchar(100) NOT NULL DEFAULT '',
  `color` varchar(100) NOT NULL DEFAULT '',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8;